/*
	Makes a really cool square! Also prints the square
	of the first number that the user inputs (except if the 
	user inputs 65)
*/
int i;
int j;

char makeAFunSquare(char length, char height) {
	int throne;
	throne = 0;
	i = 0;

	while (i < height) {
		j = 0;
		while (j < length) {
			if (j == throne) {
				write 1;
			} else {
				write 0;
			}
			j = j + 1;
		}
		writeln;
		throne = throne + 1;
		i = i + 1;
	}
}

int assignArray(char x[], char a,  char b) {
